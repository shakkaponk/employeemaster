context employee {
    type BusinessKey : String(5000);

    entity employee {
            FIRSTNAME  : BusinessKey;
            LASTNAME   : BusinessKey;
            EMAIL      : BusinessKey;
            CITY       : BusinessKey;
            JOBTITLE   : BusinessKey;
            DEPARTMENT : BusinessKey;
            key EMPLOYEEID : Integer generated by default as identity;
            
    }
    technical configuration {
        column store;
    };
};